<%-include('../navbar.ejs')%>
<%-include('../header.ejs')%>


<div class="container bg-white p-3">
    <div class="row">
        <div class="container-fluid heading">
            <div class="row">   
                <div class="col-md-12">
                    <h2>User Inquiry List</h2>
                </div>
            </div>
        </div>
        <div class="col-md-12 mt-5 ">
            <table class="table table-bordered table-shriped text-center ">
                <tr>
                    <th>Action</th>
                    <th>S.N.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Address</th>
                    <th>Message</th>
                </tr>
                <% for(var i=0;i<inquiry_list_data.length;i++) { %>
                    <tr>
                        <td>
                            <a href="/delete_inquiry_list/<%=inquiry_list_data[i].inquiry_list_id%>" onclick="return confirm('ARE YOU SURE...?')"> <button class="btn btn-sm bg-danger"><i class="fa fa-trash text-white"></i></button></a>

                        </td>
                        <td><%=i+1%></td>
                            <td><%=inquiry_list_data[i].name%></td>
                            <td><%=inquiry_list_data[i].email%></td>
                            <td><%=inquiry_list_data[i].phone%></td>
                            <td><%=inquiry_list_data[i].address%></td>
                            <td><%=inquiry_list_data[i].message%></td>
                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</div>

<%-include('../footer.ejs')%>