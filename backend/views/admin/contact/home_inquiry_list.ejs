<%-include('../navbar.ejs')%>
<%-include('../header.ejs')%>


<div class="container bg-white p-3">
    <div class="row">
        <div class="container-fluid heading">
            <div class="row">   
                <div class="col-md-12">
                    <h2>User Inquiry List</h2>
                </div>
            </div>
        </div>
        <div class="col-md-12 mt-5 ">
            <table class="table table-bordered table-shriped text-center ">
                <tr>
                    <th>Action</th>
                    <th>S.N.</th>
                    <th>Name</th>
                    <th>Phone</th>
                    <th>Email</th>
                    <th>Address</th>
                </tr>
                <% for(var i=0;i<inquiry.length;i++) { %>
                    <tr>
                        <td>
                            <a href="/delete_home_inquiry_list/<%=inquiry[i].home_inquiry_id%>" onclick="return confirm('ARE YOU SURE...?')"> <button class="btn bg-danger btn-sm"><i class="fa fa-trash text-white"></i></button></a>

                        </td>
                        <td><%=i+1%></td>
                            <td><%=inquiry[i].home_inquiry_name%></td>
                            <td><%=inquiry[i].home_inquiry_mobile%></td>
                            <td><%=inquiry[i].home_inquiry_email%></td>
                            <td><%=inquiry[i].home_inquiry_address%></td>
                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</div>

<%-include('../footer.ejs')%>